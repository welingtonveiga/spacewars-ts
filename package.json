{
  "name": "spacewars",
  "version": "0.1.0",
  "description": "A TypeScript RxJS study, based on 'Spaceship Reactive' drom Reactive Programming with RxJS",
  "author": "Welington Veiga <welington.veiga@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "types": "dist/src/main.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "karma start --singleRun true",
    "test:ci": "karma start --singleRun true --browsers 'PhantomJS'",
    "test:watch": "karma start --singleRun false",
    "build": "webpack --watch",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "posttest": "istanbul check-coverage --statement 90 --branch 90 --function 90",
    "clean": "rimraf dist node_modules",
    "server": "webpack-dev-server --progress --profile --watch --content-base src/"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-rc.4",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.35",
    "es6-shim": "^0.35.1"
  },
  "devDependencies": {
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.24.1",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "object-assign": "^4.1.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.9.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.5.0",
    "typedoc-webpack-plugin": "^1.0.10",
    "typescript": "^2.0.3",
    "watchify": "^3.7.0",
    "webpack": "^1.13.0",
    "webpack-split-by-path": "0.0.10"
  }
}
